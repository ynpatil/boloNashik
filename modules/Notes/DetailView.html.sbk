<!-- BEGIN: portal_on -->
	<tr>
	<td class='tabDetailViewDL'><span sugar='slot10'>&nbsp;</span sugar='slot'></td>
	<td class='tabDetailViewDF'><span sugar='slot10b'>&nbsp;</span sugar='slot'></td>
	<td class='tabDetailViewDL'><span sugar='slot11'>{MOD.LBL_PORTAL_FLAG}</span sugar='slot'></td>
	<td class='tabDetailViewDF'><span sugar='slot11b'><input name='portal_flag' disabled class='checkbox' type='checkbox' {PORTAL_FLAG}></span sugar='slot'></td>
	</tr>
<!-- END: portal_on -->

<!-- BEGIN: portal_off -->
	<tr>
	<td class='tabDetailViewDL'><span sugar='slot12'>&nbsp;</span sugar='slot'></td>
