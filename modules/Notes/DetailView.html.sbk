<!-- END: portal_off -->
&nbsp;
</div>
	<tr>
	<td width='15%' class="tabDetailViewDL"><span sugar='slot14'>{MOD.LBL_SUBJECT}</span sugar='slot'></td>
	<td colspan='3' class="tabDetailViewDF"><span sugar='slot14b'>{NAME}&nbsp;</span sugar='slot'></td>
	</tr><tr>
	<td class="tabDetailViewDL"><span sugar='slot15'>{MOD.LBL_FILENAME}</span sugar='slot'></td>
	<td colspan='3' class="tabDetailViewDF"><span sugar='slot15b'>{FILELINK}</span sugar='slot'></td>
	</tr><tr>
	<td valign="top" valign="top" class="tabDetailViewDL"><span sugar='slot16'>{MOD.LBL_NOTE}</span sugar='slot'></td>
	<td colspan="3" class="tabDetailViewDF"><span sugar='slot16b'>{DESCRIPTION}&nbsp;</span sugar='slot'></td>
</tr></table>
</td></tr>
</table>
<script>

//toggle_portal_flag();

function toggle_portal_flag() {
	if (typeof(this.document.form.portal_on) != 'undefined')
	{
	}
	else
	{
		var table = "<table width='100%' border='0' cellspacing='0' cellpadding='0'>\n";
		table += "<tr><td width='15%' class='tabDetailViewDL'><span sugar='slot17'>&nbsp;</span sugar='slot'></td>\n";
		table += "<td width='35%' class='tabDetailViewDF'><span sugar='slot17b'>&nbsp;</span sugar='slot'></td>\n";
		table += "<td width='15%' class='tabDetailViewDL'><span sugar='slot18'>&nbsp</span sugar='slot'></td>\n";
		table += "<td width='35%' class='tabDetailViewDF'><span sugar='slot18b'>&nbsp</span sugar='slot'></td>";
		table += "</tr></table>";
		if (typeof(this.document.getElementById('portal_flag_row')) != 'undefined'){
			this.document.getElementById('portal_flag_row').innerHTML = table;
			alert(table);
		}
	}

}
</script>
