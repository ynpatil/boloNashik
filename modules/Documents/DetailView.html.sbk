        <td width="15%" valign="top" class="tabDetailViewDL"><span sugar='slot15'>{MOD.LBL_DET_RELATED_DOCUMENT_VERSION}</span sugar='slot'></td>
		<td width="35%" valign="top" class="tabDetailViewDF"><span sugar='slot15b'>{RELATED_DOCUMENT_VERSION}&nbsp;</span sugar='slot'></td>		        
